# Set the paths
set project_dir "[file normalize "./project"]"
set src_dir "[file normalize "./src"]"
set project_name "dmagen3"

# Open the project
open_project $project_dir/$project_name/$project_name.xpr

# Reset stages
reset_run synth_1
reset_run impl_1

# And run them again.
launch_runs synth_1 -jobs 4
wait_on_run synth_1

launch_runs impl_1 -to_step write_bitstream -jobs 4
wait_on_run impl_1


close_project